<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<SplitPane dividerPositions="0.17" orientation="VERTICAL" prefHeight="310.0" prefWidth="362.0"
           xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="app.deyal.deyal_app.controllers.CompleteMissionController">
    <items>
        <HBox alignment="CENTER" spacing="10.0">
            <children>
                <Label text="Got your rewards from client?">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </Label>
                <ChoiceBox fx:id="gotRewardChoiceBox" prefWidth="100.0"/>
            </children>
        </HBox>
        <VBox alignment="CENTER" spacing="15.0">
            <children>
                <Label text="Enter a review">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <TextArea fx:id="messageTextArea" prefHeight="150.0" prefWidth="350.0"
                          promptText="Say how this mission was, how client behaved and if you are satisfied or not."/>
                <Button mnemonicParsing="false" onAction="#handleCompleteButtonAction" prefHeight="25.0"
                        prefWidth="70.0" text="Complete"/>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </VBox>
    </items>
</SplitPane>
