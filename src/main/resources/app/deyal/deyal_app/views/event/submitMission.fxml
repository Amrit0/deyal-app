<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<SplitPane dividerPositions="0.1640625" orientation="VERTICAL" prefHeight="250.0" prefWidth="362.0"
           xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="app.deyal.deyal_app.controllers.SubmitMissionController">
    <VBox alignment="CENTER">
        <Label text="Give proof of work">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
    </VBox>
    <VBox alignment="CENTER" spacing="10.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <TextArea fx:id="messageTextArea" prefHeight="150.0" prefWidth="350.0"
                  promptText="Include what you did, the process and final result."
                  text="Process of finishing the mission:&#10;&#10;Final result:"/>
        <Button mnemonicParsing="false" onAction="#handleSubmitButtonAction" prefHeight="25.0" prefWidth="70.0"
                text="Submit"/>
    </VBox>
</SplitPane>
