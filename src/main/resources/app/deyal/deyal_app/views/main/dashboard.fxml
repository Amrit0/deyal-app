<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="root" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.deyal.deyal_app.controllers.DashboardController">
    <ScrollPane>
        <content>
            <TableView fx:id="dashboardTableView" prefHeight="600.0" prefWidth="700.0" tableMenuButtonVisible="true">
                <columns>
                    <TableColumn fx:id="missionTitleTableColumn" prefWidth="120.0" text="Mission Name"/>
                    <TableColumn fx:id="missionLevelTableColumn" prefWidth="100.0" text="Level"/>
                    <TableColumn fx:id="missionCreatorTableColumn" prefWidth="120.0" text="Contractor Name"/>
                    <TableColumn fx:id="missionDescriptionTableColumn" prefWidth="300.0" text="Short Description"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
        </content>
    </ScrollPane>
</AnchorPane>
