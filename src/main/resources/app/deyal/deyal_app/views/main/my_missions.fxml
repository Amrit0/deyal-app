<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="root" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.deyal.deyal_app.controllers.MyMissionsController">
    <ScrollPane>
        <TableView fx:id="myMissionTableView" prefHeight="600.0" prefWidth="700.0" tableMenuButtonVisible="true">
            <columns>
                <TableColumn fx:id="mmMissionTitleTableColumn" prefWidth="120.0" text="Mission Name"/>
                <TableColumn fx:id="mmMissionStatusTableColumn" prefWidth="120.0" text="Status"/>
                <TableColumn fx:id="mmMissionLevelTableColumn" prefWidth="100.0" text="Level"/>
                <TableColumn fx:id="mmMissionDescriptionTableColumn" prefWidth="250.0" text="Short Description"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </ScrollPane>
</AnchorPane>
